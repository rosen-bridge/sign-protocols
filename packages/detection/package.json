{
  "name": "@rosen-bridge/detection",
  "version": "1.0.0",
  "description": "find available endpoints in private network",
  "repository": "https://github.com/rosen-bridge/sign-protocols",
  "license": "GPL-3.0",
  "author": "Rosen Team",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "coverage": "npm run test -- --coverage --run",
    "lint": "eslint --fix . && npm run prettify",
    "prettify": "prettier --write . --ignore-path ./.gitignore",
    "release": "npm run test -- --run && npm run build && npm publish --access public",
    "test": "NODE_OPTIONS='--import tsx' vitest",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.11.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-istanbul": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@rosen-bridge/abstract-logger": "^2.0.1",
    "@rosen-bridge/communication": "^1.0.0",
    "@rosen-bridge/encryption": "^0.1.0"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
